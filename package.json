{
  "name": "typescript-language-server",
  "version": "1.1.2",
  "description": "Language Server Protocol (LSP) implementation for TypeScript using tsserver",
  "author": "TypeFox and others",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/typescript-language-server/typescript-language-server.git"
  },
  "engines": {
    "node": ">=14.16"
  },
  "bin": "./out/cli.js",
  "main": "./out/server.js",
  "typings": "./out/server.d.ts",
  "scripts": {
    "pre:clean":  "rimraf lib *.tsbuildinfo out", 
    "clean": "npm run pre:clean && tsc -b && chmod 755 ./out/cli.js",
    "prepare:tests": "rm -f test_data/fish_files/*.fish && yarn run prepare:install:tests",
    "prepare:install:tests": "cd test_data && ./install_scripts/generate_largest_fish_files.fish && cd ..",
    "test": "jest --runInBand --watch ",
    "compile": "tsc -b",
    "watch": "npm run clean && tsc -b --watch --verbose"
  },
  "eslintIgnore": [
    "!.eslintrc.cjs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn run clean",
      "post-merge": "yarn"
    }
  },
  "dependencies": {
    "@types/globby": "^9.1.0",
    "commander": "^9.4.0",
    "deepmerge": "^4.2.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "git": "^0.1.5",
    "glob": "^8.0.3",
    "global": "^4.4.0",
    "globby": "^13.1.2",
    "pkg-up": "^4.0.0",
    "semver": "^7.3.7",
    "tree-sitter-fish": "github:ram02z/tree-sitter-fish",
    "ts-jest": "^29.0.1",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-protocol": "^3.17.2",
    "vscode-languageserver-textdocument": "1.0.7",
    "vscode-uri": "^3.0.6",
    "web-tree-sitter": "^0.20.7"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.0.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.11.56",
    "@types/semver": "^7.3.12",
    "@types/shell-quote": "^1.7.1",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "chai": "^4.3.6",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "husky": "^4.3.8",
    "jest": "^29.0.3",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "shell-quote": "^1.7.4",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "enabledApiProposals": [
    "inlineCompletions"
  ]
}
